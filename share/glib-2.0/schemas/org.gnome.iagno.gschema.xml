<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/org/gnome/iagno/" id="org.gnome.iagno" gettext-domain="gnome-games">
    <key type="i" name="black-level">
      <default>0</default>
      <range min="0" max="3"/>
    </key>
    <key type="i" name="white-level">
      <default>1</default>
      <range min="0" max="3"/>
    </key>
    <key type="b" name="quick-moves">
      <default>false</default>
    </key>
    <key type="s" name="tileset">
      <default>'classic.png'</default>
    </key>
    <key type="i" name="animate">
      <default>2</default>
      <range min="0" max="2"/>
    </key>
    <key type="b" name="animate-stagger">
      <default>false</default>
    </key>
    <key type="b" name="show-grid">
      <default>false</default>
    </key>
    <key type="b" name="flip-final-results">
      <default>true</default>
    </key>
    <key type="b" name="sound">
      <default>true</default>
      <summary>Sound</summary>
      <description>Whether or not to play event sounds.</description>
    </key>
  </schema>
</schemalist>